{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "abbreviation": {
          "type": "string"
        },
        "game": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "isbn": {
          "type": "string",
          "pattern": "^\\d{3}-\\d{1}-\\d{5}-\\d{3}-\\d{1}$"
        },
        "sku": {
          "type": "string"
        }
      },
      "required": ["name", "description", "abbreviation", "game", "type", "isbn", "sku"]
    }
  }
  